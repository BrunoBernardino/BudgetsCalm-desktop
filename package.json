{
  "name": "budgetscalm-desktop",
  "productName": "Budgets, Calm",
  "version": "1.0.0",
  "build": "JBXCVMwB",
  "description": "Desktop app for Budgets, Calm.",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package --platform=mas",
    "make": "electron-forge make --platform=mas",
    "sign": "electron-osx-sign 'out/Budgets, Calm-mas-x64/Budgets, Calm.app' --provisioning-profile='BudgetsCalm-macOS.provisionprofile' --entitlements='entitlements.plist' --platform=mas --version=7.1.14 --type=distribution",
    "flat": "electron-osx-flat 'out/Budgets, Calm-mas-x64/Budgets, Calm.app' --platform=mas",
    "publish": "electron-forge publish",
    "test": "jest",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src/ *.js",
    "pretty": "prettier --write '*.{js,jsx,ts,tsx,json,scss}' 'src/**/*.{js,jsx,ts,tsx,json,scss}'",
    "pretty/test": "prettier --check '*.{js,jsx,ts,tsx,json,scss}' 'src/**/*.{js,jsx,ts,tsx,json,scss}'"
  },
  "private": true,
  "engines": {
    "node": "12.x"
  },
  "keywords": [],
  "author": {
    "name": "Bruno Bernardino",
    "email": "me@brunobernardino.com"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "public/icon.icns",
        "platform": "mas",
        "overwrite": true,
        "appBundleId": "com.emotionloop.BudgetsCalm-macOS",
        "appCategoryType": "public.app-category.finance",
        "appCopyright": "Bruno Bernardino",
        "buildVersion": 4
      },
      "makers": [
        {
          "name": "@electron-forge/maker-pkg"
        },
        {
          "name": "@electron-forge/maker-dmg"
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/app.tsx",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/out/"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "7.8.3",
    "@babel/preset-react": "7.8.3",
    "@babel/preset-typescript": "7.8.3",
    "@electron-forge/cli": "6.0.0-beta.49",
    "@electron-forge/maker-dmg": "6.0.0-beta.49",
    "@electron-forge/maker-pkg": "6.0.0-beta.49",
    "@electron-forge/plugin-webpack": "6.0.0-beta.49",
    "@marshallofsound/webpack-asset-relocator-loader": "0.5.0",
    "@types/jest": "24.9.1",
    "@types/react": "16.9.19",
    "@types/react-dom": "16.9.5",
    "@types/react-test-renderer": "16.9.2",
    "@types/styled-components": "4.4.2",
    "@typescript-eslint/eslint-plugin": "2.17.0",
    "@typescript-eslint/parser": "2.17.0",
    "babel-jest": "25.1.0",
    "css-loader": "3.4.2",
    "electron": "7.1.14",
    "electron-store": "5.1.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.9.0",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.18.0",
    "fork-ts-checker-webpack-plugin": "4.0.3",
    "jest": "25.1.0",
    "memdown": "5.1.0",
    "moment": "2.24.0",
    "node-loader": "0.6.0",
    "node-sass": "4.13.1",
    "pouchdb-adapter-http": "7.1.1",
    "pouchdb-adapter-idb": "7.1.1",
    "pouchdb-erase": "1.0.2",
    "prettier": "1.19.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-svg-ionicons": "0.2.3",
    "react-test-renderer": "16.12.0",
    "react-toggle-switch": "3.0.4",
    "rodal": "1.7.0",
    "rxdb": "8.7.5",
    "rxjs": "6.5.4",
    "sass-loader": "8.0.2",
    "style-loader": "1.1.3",
    "styled-components": "5.0.0",
    "ts-loader": "6.2.1",
    "typescript": "3.7.5",
    "url-loader": "3.0.0"
  },
  "dependencies": {
    "electron-squirrel-startup": "1.0.0"
  }
}
